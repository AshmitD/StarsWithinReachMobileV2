{
  "expo": {
    "name": "StarsWithinReachMobileV4",
    "slug": "StarsWithinReachMobileV4",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ashmitdewan.starswithinreach",
          "infoPlist": {
            "NSPhotoLibraryAddUsageDescription": "This app takes user-selected photos from the photo library, saves them in the database and displays them in the 'feed' feature."
          }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.ashmitdewan.starswithinreach"
    },
    "sdkVersion": "35.0.0"
  }

}